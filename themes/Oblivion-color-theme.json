{
    "name": "Oblivion",
    "type": "dark",
    "colors": {
        "editor.background": "#1E1E1E",
        "editorCursor.foreground": "#F8F8F0",
        "editor.foreground": "#D8D8D8",
        "editorWhitespace.foreground": "#3B3A32",
        "editor.lineHighlightBackground": "#3E3D32",
        "editor.selectionBackground": "#49483E"
    },
    "tokenColors": [
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
        {
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
        {
            "name": "Editor",
            "scope": "editor.foreground",
            "settings": {
                "foreground": "#D8D8D8"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#C7DD0C",
                "fontStyle": " italic"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#FFC600"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#7FB347"
            }
        },
        {
            "name": "Number Foreground",
            "scope": "symbolIcon.numberForeground",
            "settings": {
                "foreground": "#7FB347"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#E32D2D"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FF7300"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#17F3FF"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": " bold"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#E32D2D"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.class",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#FFFFFF",
                "fontStyle": " italic"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#D8D8D8"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#bd3220"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#D8D8D8"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#D8D8D8"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#F8F8F0"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#F8F8F0"
            }
        },      
    ]
}